    "lucide-react": "^0.456.0",
    "next-language-detector": "^1.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^15.1.1",
    "react-router-dom": "7.0.1",
    "react-router-typesafe-routes": "^2.0.0",
    "react-use": "^17.5.1",
