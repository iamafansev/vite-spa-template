    "typescript": "5.8.2"
  },
  "devDependencies": {
    "@eslint/js": "9.21.0",
    "@graphql-codegen/client-preset": "^4.6.4",
    "@tailwindcss/postcss": "^4.0.9",
    "@tanstack/eslint-plugin-router": "^1.105.0",
