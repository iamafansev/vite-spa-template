{
  "name": "vite-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "tsc:check": "tsc -b",
    "unit": "vitest --run",
    "unit:watch": "vitest",
    "preview": "vite preview",
    "docker:build": "tsc && docker build -t docker-react .",
